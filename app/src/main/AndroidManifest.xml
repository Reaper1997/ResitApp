<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.steven.resitapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ClothesCategory" />
        <activity android:name=".Shoes" />
        <activity android:name=".Tshirts" />
        <activity android:name=".Watches" />
        <activity android:name=".Jeans" />
        <activity android:name=".Sunglasses" />
        <activity android:name=".LoginProfile"></activity>
    </application>

</manifest>